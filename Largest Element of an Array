// C Program To Find Largest Element of an Array
#include <stdio.h>
int main(){
    int i, largest;
    int arr[10] = {1, 2, 3, 4, 5, 6, 7, 8, 9, 0};
    
    largest = arr[0];
    
    for (i = 0; i < 10; i++){
        if (largest < arr[i]){
            largest = arr[i];
        }
    }
    printf("Largest Element of the array is: %d", largest);
    return 0;
}
